{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Development Server",
      "command": "./node_modules/.bin/astro dev",
      "request": "launch",
      "type": "node-terminal",
      "presentation": {
        "hidden": true
      }
    },
    {
      "name": "Chromium Debug",
      "type": "chrome",
      "request": "launch",
      "runtimeExecutable": "/Applications/Chromium.app/Contents/MacOS/Chromium",
      "userDataDir": "${env:HOME}/Library/Application Support/Chromium Debug",
      "url": "http://localhost:4321",
      "presentation": {
        "hidden": true
      }
    }
  ],
  "compounds": [
    {
      "name": "Debugging",
      "configurations": [
        "Development Server",
        "Chromium Debug"
      ]
    }
  ]
}
